{
  "name": "react-sequencer",
  "version": "0.1.0",
  "description": "Attempt at using React for this sequencer prototype!",
  "scripts": {
    "prestart": "npm install",
    "start": ". ./run-start-local.sh",
    "start-windows": "set API_URL='http://localhost:5000'&&set NODE_ENV='development'&&node server.js",
    "start-prod": ". ./run-start-prod.sh",
    "build-local": ". ./run-build-local.sh",
    "build-prod": ". ./run-build-prod.sh",
    "test": "find ./src -name '*.test.js' | env API_URL='\"http://localhost:5000\"' NODE_ENV=test NODE_PATH=./src xargs mocha --require babel-core/register --compilers jpg:./mocha.config.js --compilers png:./mocha.config.js --compilers ico:./mocha.config.js -r mock-local-storage",
    "test-watch": "find ./src -name '*.test.js' | env API_URL=$'\"http://localhost:5000\"' NODE_ENV=test NODE_PATH=./src xargs mocha --require babel-core/register --compilers jpg:./mocha.config.js --compilers png:./mocha.config.js --compilers ico:./mocha.config.js -r mock-local-storage -w",
    "lint": "eslint src",
    "lint-fix": "eslint --fix src",
    "new-plugin": "node bin/new-plugin.js"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
